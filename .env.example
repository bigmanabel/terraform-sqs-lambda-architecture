# AWS Credentials (alternative to AWS CLI profiles)
export AWS_ACCESS_KEY_ID=your_access_key_here
export AWS_SECRET_ACCESS_KEY=your_secret_key_here
export AWS_DEFAULT_REGION=us-east-1

# Optional: AWS Profile (alternative to explicit credentials)
# export AWS_PROFILE=your-profile-name

# Terraform Variables (can also be set in terraform.tfvars)
export TF_VAR_aws_region=us-east-1
export TF_VAR_project_name=event-pipeline
export TF_VAR_github_owner=your-github-username
export TF_VAR_github_repo=your-lambda-functions-repo
export TF_VAR_github_branch=main